<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lemon.mapper.TTravelFoodMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lemon.entity.TTravelFood">
        <id column="food_id" property="foodId" />
        <result column="food_name" property="foodName" />
        <result column="food_img" property="foodImg" />
        <result column="food_category" property="foodCategory" />
        <result column="snic_name" property="snicName" />
        <result column="food_price" property="foodPrice" />
        <result column="food_remark" property="foodRemark" />
        <result column="food_hot" property="foodHot" />
        <result column="rut_time" property="rutTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        food_id, food_name, food_img, food_category, snic_name, food_price, food_remark, food_hot, rut_time
    </sql>

</mapper>
